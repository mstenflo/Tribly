rc/components/contributions/ContributionItem.js",["228","229","230","231"],"import React, { useState } from 'react'\nimport PropTypes from 'prop-types'\nimport ReactPlayer from 'react-player'\nimport ImgsViewer from 'react-images-viewer'\n\nconst ContributionItem = ({ contribution: { title, text, file, author, filetype, youtube, link } }) => {\n  const [currentImage, setCurrentImage] = useState(0);\n  const [viewerIsOpen, setViewerIsOpen] = useState(false);\n\n  const openLightbox = () => {\n    setViewerIsOpen(true);\n  };\n\n  const closeLightbox = () => {\n    setCurrentImage(0);\n    setViewerIsOpen(false);\n  };\n\n  return (\n    <div>\n      <h1 className=\"text-primary\">{title}</h1>\n      <h3 className=\"mb-1\">{author.name}</h3>\n      {\n        !filetype ? null :\n        ReactPlayer.canPlay(file) ? \n          <ReactPlayer url={file} controls /> : filetype === 'youtube' ?\n            <div>\n              <ReactPlayer url={youtube} controls className=\"mb-1\" />\n              <a href={youtube} className=\"btn btn-primary\" target=\"_blank\" rel=\"noopener noreferrer\">\n                Click Me!\n              </a> to go to the website\n            </div> : filetype === 'image/*' ? \n              <div>\n                <div className=\"contribution-image-container\" onClick={() => setViewerIsOpen(true)}>\n                  <img src={file} alt=\"image\" onClick={() => setViewerIsOpen(true)} />\n                  <ImgsViewer\n                    imgs={[{ src: file }]}\n                    showThumbnails\n                    enableKeyboardInput\n                    width=\"50vw\"\n                    isOpen={viewerIsOpen}\n                    onClose={() => setViewerIsOpen(false)}\n                    backdropCloseable \n                  />\n                </div>\n              </div> : <a href={link} className=\"btn btn-primary\" target=\"_blank\" rel=\"noopener noreferrer\">Click Me!</a>\n      }\n      <div className=\"mb-1\"></div>\n      <p>{text}</p>\n    </div>\n  )\n}\n\nContributionItem.propTypes = {\n  contribution: PropTypes.object.isRequired,\n}\n\nexport default ContributionItem\n","/Users/martinstenflo/Software/Tribly/client/src/store.js",[],["232","233"],"/Users/martinstenflo/Software/Tribly/client/src/components/layout/Alert.js",[],["234","235"],"/Users/martinstenflo/Software/Tribly/client/src/components/profiles/Profiles.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/groups/Groups.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/profile-forms/CreateProfile.js",[],"/Users/martinstenflo/Software/Tribly/client/src/actions/alert.js",[],"/Users/martinstenflo/Software/Tribly/client/src/actions/profile.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/profile/ProfileAbout.js",[],"/Users/martinstenflo/Software/Tribly/client/src/config.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/comment-forms/CommentForm.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/topics/TopicItem.js",[],"/Users/martinstenflo/Software/Tribly/client/src/reducers/auth.js",[],"/Users/martinstenflo/Software/Tribly/client/src/index.js",[],"/Users/martinstenflo/Software/Tribly/client/src/actions/auth.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/routing/PrivateRoute.js",[],["236","237"],"/Users/martinstenflo/Software/Tribly/client/src/components/layout/Navbar.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/group-forms/GroupForm.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/auth/Register.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/layout/Spinner.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/groups/GroupItem.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/profile/ProfileTop.js",[],"/Users/martinstenflo/Software/Tribly/client/src/reducers/index.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/topic-forms/TopicForm.js",[],"/Users/martinstenflo/Software/Tribly/client/src/reducers/profile.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/profiles/ProfileItem.js",[],["238","239"],"/Users/martinstenflo/Software/Tribly/client/src/components/profile-forms/EditProfile.js",["240"],"import React, { Fragment, useState, useEffect } from 'react';\nimport { Link, withRouter } from 'react-router-dom';\nimport PropTypes from 'prop-types';\nimport S3FileUpload from 'react-s3';\nimport S3Config from '../../config';\nimport { connect } from 'react-redux';\nimport { createProfile, getCurrentProfile } from '../../actions/profile';\n\nconst EditProfile = ({\n  profile: { profile, loading },\n  createProfile,\n  getCurrentProfile,\n  history,\n}) => {\n  const [formData, setFormData] = useState({\n    website: '',\n    avatar: '',\n    location: '',\n    skills: '',\n    bio: '',\n    twitter: '',\n    facebook: '',\n    youtube: '',\n    instagram: '',\n  });\n\n  const [displaySocialInputs, toggleSocialInputs] = useState(false);\n\n  const config = {\n    bucketName: S3Config.S3Bucket,\n    region: S3Config.S3Region,\n    accessKeyId: S3Config.S3AccessKeyID,\n    secretAccessKey: S3Config.S3SecretAccessKey\n  }\n  \n  useEffect(() => {\n    getCurrentProfile();\n\n    setFormData({\n      website: loading || !profile.website ? '' : profile.website,\n      avatar: loading || !profile.avatar ? '' : profile.avatar,\n      location: loading || !profile.location ? '' : profile.location,\n      skills: loading || !profile.skills ? '' : profile.skills.join(', '),\n      bio: loading || !profile.bio ? '' : profile.bio,\n      twitter: loading || !profile.social ? '' : profile.social.twitter,\n      facebook: loading || !profile.social ? '' : profile.social.facebook,\n      youtube: loading || !profile.social ? '' : profile.social.youtube,\n      instagram: loading || !profile.social ? '' : profile.social.instagram,\n    });\n  }, [loading, getCurrentProfile]);\n\n  const {\n    website,\n    avatar,\n    location,\n    skills,\n    bio,\n    twitter,\n    facebook,\n    youtube,\n    instagram,\n  } = formData;\n\n  const onChange = (e) => {\n    if (e.target.files) {\n      S3FileUpload.uploadFile(e.target.files[0], config, { mode: 'no-cors' })\n        .then(data => {\n          setFormData({ ...formData, avatar: data.location });\n        })\n        .catch(err => {\n          alert(err);\n        });\n    } else {\n      if (e.target.name === 'website') {\n        const weblink = e.target.value.includes('http') ? e.target.value : 'http://' + e.target.value;\n        setFormData({ ...formData, website: weblink });\n      } else {\n        setFormData({ ...formData, [e.target.name]: e.target.value });\n      }\n    }\n  }\n\n  const onSubmit = (e) => {\n    e.preventDefault();\n    createProfile(formData, history, true);\n  };\n\n  return (\n    <Fragment>\n      <div className=\"flexit\">\n        <h1 className='large text-primary'>Edit Your Profile</h1>\n        <div className=\"avatar\">\n          {\n            !avatar ?\n              <img src={`https://tribly.s3-us-west-1.amazonaws.com/avatar_default.png`} alt=\"avatar\" /> : \n              <img src={avatar} id=\"avatar\" alt=\"avatar\"/>\n          }\n          <input\n            type=\"file\"\n            name=\"profilepic\"\n            id=\"profilepic\"\n            accept=\"image/*\"\n            onChange={e => onChange(e)}\n            placeholder=\"add an image\" />\n        </div>\n      </div>\n      <p className='lead'>\n        <i className='fas fa-user' /> Add some changes to your profile\n      </p>\n      <form className='form' onSubmit={(e) => onSubmit(e)}>\n        <div className='form-group'>\n          <input\n            type='text'\n            placeholder='Website'\n            name='website'\n            value={website}\n            onChange={(e) => onChange(e)}\n          />\n          <small className='form-text'>\n            Could be your own or a company website\n          </small>\n        </div>\n        <div className='form-group'>\n          <input\n            type='text'\n            placeholder='Location'\n            name='location'\n            value={location}\n            onChange={(e) => onChange(e)}\n          />\n          <small className='form-text'>\n            City & state suggested (eg. Boston, MA)\n          </small>\n        </div>\n        <div className='form-group'>\n          <input\n            type='text'\n            name='skills'\n            value={skills}\n            onChange={(e) => onChange(e)}\n          />\n          <small className='form-text'>\n            Please use comma separated values (eg. HTML,CSS,JavaScript,PHP)\n          </small>\n        </div>\n        <div className='form-group'>\n          <textarea\n            placeholder='A short bio of yourself'\n            name='bio'\n            value={bio}\n            onChange={(e) => onChange(e)}\n          />\n          <small className='form-text'>Tell us a little about yourself</small>\n        </div>\n\n        <div className='my-2'>\n          <button\n            onClick={() => toggleSocialInputs(!displaySocialInputs)}\n            type='button'\n            className='btn btn-light'\n          >\n            Add Social Network Links\n          </button>\n          <span>Optional</span>\n        </div>\n\n        {displaySocialInputs && (\n          <Fragment>\n            <div className='form-group social-input'>\n              <i className='fab fa-twitter fa-2x' />\n              <input\n                type='text'\n                placeholder='Twitter URL'\n                name='twitter'\n                value={twitter}\n                onChange={(e) => onChange(e)}\n              />\n            </div>\n\n            <div className='form-group social-input'>\n              <i className='fab fa-facebook fa-2x' />\n              <input\n                type='text'\n                placeholder='Facebook URL'\n                name='facebook'\n                value={facebook}\n                onChange={(e) => onChange(e)}\n              />\n            </div>\n\n            <div className='form-group social-input'>\n              <i className='fab fa-youtube fa-2x' />\n              <input\n                type='text'\n                placeholder='YouTube URL'\n                name='youtube'\n                value={youtube}\n                onChange={(e) => onChange(e)}\n              />\n            </div>\n\n            <div className='form-group social-input'>\n              <i className='fab fa-instagram fa-2x' />\n              <input\n                type='text'\n                placeholder='Instagram URL'\n                name='instagram'\n                value={instagram}\n                onChange={(e) => onChange(e)}\n              />\n            </div>\n          </Fragment>\n        )}\n\n        <input type='submit' className='btn btn-primary my-1' />\n        <Link className='btn btn-light my-1' to='/dashboard'>\n          Go Back\n        </Link>\n      </form>\n    </Fragment>\n  );\n};\n\nEditProfile.propTypes = {\n  createProfile: PropTypes.func.isRequired,\n  getCurrentProfile: PropTypes.func.isRequired,\n  profile: PropTypes.object.isRequired,\n};\n\nconst mapStateToProps = state => ({\n  profile: state.profile,\n});\n\nexport default connect(mapStateToProps, { createProfile, getCurrentProfile })(\n  withRouter(EditProfile)\n);","/Users/martinstenflo/Software/Tribly/client/src/components/comments/CommentItem.js",[],"/Users/martinstenflo/Software/Tribly/client/src/utils/setAuthToken.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/dashboard/Dashboard.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/group/Group.js",[],"/Users/martinstenflo/Software/Tribly/client/src/components/topics/Topics.js",["241"],"import React, { Fragment } from 'react'\nimport PropTypes from 'prop-types'\nimport TopicItem from './TopicItem'\n\nconst